<%-
  record_sep = if @package == 'strongswan' then ' : ' else ': ' end

  # sort wildcard (%any) records to the bottom of the file
  sorted_cfg = @config.reject{ |k,v| k.include? '%any' }
  sorted_cfg.merge!(@config.select{ |k,v| k.include? '%any' })

  sorted_cfg.each do |k,v|
    next if v.nil?
-%>
<%= k %><%= record_sep %><%= v %>
<%- end -%>

